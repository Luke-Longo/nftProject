{
	"compilerOptions": {
		"target": "es2017",
		"lib": ["es6"],
		"module": "es2022",
		"moduleResolution": "node",
		"rootDir": "src",
		"resolveJsonModule": true,
		"allowJs": true,
		"outDir": "build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	}
}
